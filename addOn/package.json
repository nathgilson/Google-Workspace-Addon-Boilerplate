{
  "name": "Google-Workspace-Addon-Boilerplate",
  "version": "2.1.0",
  "description": "Google Apps Script Boilerplate",
  "scripts": {
    "login": "clasp login",
    "setup": "rm -rf .clasp.json && clasp create --type standalone --title 'My Google Workspace Addon' && clasp setting rootDir ./dist",
    "setup:use-id": "clasp setting scriptId",
    "build": "webpack",
    "deploy": "rm -rf dist && npm run build && npx clasp push"
  },
  "author": "nathgilson",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
